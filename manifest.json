{
  "manifest_version": 3,
  "name": "Sea Turtle",
  "version": "1.0.0",
  "description": "AI powered super SeaTalk.",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://seatalkweb.com/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}